        "fraud_type"
      ]
    },
    "insurance_influence": {
      "required_fields": [
        "insurer_id",
        "total_contributions",
        "correlation_coefficient",
        "influence_risk_score"
      ],
      "optional_fields": [
        "legislators_receiving",
        "votes_correlated",
        "rate_benefit_detected"
      ]
    },
    "sheriff_contract": {
      "required_fields": [
        "contract_id",
        "sheriff_office",
        "contractor",
        "monthly_amount",
        "fraud_risk_score"
      ],
      "optional_fields": [
        "deliverables_documented",
        "personal_tie_detected",
        "kickback_suspected",
        "gambling_flag"
