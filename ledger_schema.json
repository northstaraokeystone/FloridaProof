    "pressure": {
      "required_fields": [
        "pressure_level",
        "detection_baseline",
        "detection_under_pressure",
        "alpha"
      ],
      "optional_fields": [
        "probes_halted",
        "subpoenas_ignored"
      ]
    },
    "axiom": {
      "required_fields": [
        "flow_id",
        "mdl",
        "compression_ratio",
        "fraud_score"
      ],
      "optional_fields": [
        "original_bits",
        "compressed_bits",
        "structural_anomaly"
      ]
    },
    "simulation": {
      "required_fields": [
        "cycles_completed",
        "all_passed"
      ],
      "optional_fields": [
        "detection_rates",
        "alpha",
        "watchers_spawned",
        "receipt_hash"
      ]
    },
    "anomaly": {
      "required_fields": [
        "metric",
        "classification",
        "action"
      ],
      "optional_fields": [
        "baseline",
        "delta",
        "expected",
        "actual"
      ]
    },
    "violation": {
      "required_fields": [
        "metric",
        "rate",
        "threshold"
      ],
      "optional_fields": [
        "classification",
        "action"
      ]
    }
  },
  "common_fields": {
    "receipt_type": "string (required)",
    "ts": "ISO8601 timestamp (required)",
    "tenant_id": "string (required)",
    "payload_hash": "sha256:blake3 format (required)"
  }
