        "network_id"
      ]
    },
    "cascade": {
      "required_fields": [
        "source_domain",
        "affected_domains",
        "systemic_risk"
      ],
      "optional_fields": [
        "cascade_path",
        "total_propagation"
      ]
    },
    "network": {
      "required_fields": [
        "entity",
        "protection_score"
      ],
      "optional_fields": [
        "connected_donors",
        "connected_politicians",
        "audit_resistance",
        "exposure_threshold"
